import javax.swing.*;

public class Multiplikationstabell {
  public static void main (String[] arg) {
  String s = JOptionPane.showInputDialog("Hur många rader multiplikationstabell vill du skriv ut?");
  int rader = Integer.parseInt(s);
  String disp = "";
  for (int a = 1; a <= rader; a++) {
   for (int b = 1; b <= a; b++)
    disp = disp + b * a + " ";
   disp = disp + "\n"; 
  }
 JOptionPane.showMessageDialog(null, disp);
 JOptionPane.showMessageDialog(null, "Fin Javafil Av Oskar Söderberg");
 }
}
